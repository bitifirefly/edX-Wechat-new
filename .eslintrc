{
  "extends": "eslint:recommended",

  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },

  "rules": {
    "no-console": off,
    "no-unused-vars": "warn",
    "indent": [ "error", 2 ],
    "quotes": [ "error", "single" ],
    "semi": [ "error", "always" ]
  }
}
