{
  "extends": "eslint:recommended",

  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },

  "rules": {
    "no-console": 0,
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
